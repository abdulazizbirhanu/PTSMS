@model IEnumerable<PTSMSDAL.Models.Scheduling.Relations.EquipmentCertificate>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CertificateType.Type)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EquipmentId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CertificateType.Type)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EquipmentId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Description)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EquipmentLicenseId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EquipmentLicenseId }) |
